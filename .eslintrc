{
   "extends": [
     "eslint:recommended",
     "plugin:react/recommended",
     "airbnb",
     "prettier"
   ],
   "plugins": [
     "react",
     "prettier"
   ],
   "rules": {
     "semi": 0,
     "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
   },
   "env": {
      "browser": true,
      "node": true
    }
 }